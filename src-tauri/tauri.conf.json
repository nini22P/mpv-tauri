{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "mpv-tauri",
  "version": "0.1.0",
  "identifier": "nini22P.mpv-tauri",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "MPV Tauri",
        "width": 1280,
        "height": 720,
        "transparent": true,
        "center": true
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  },
  "plugins": {
    "cli": {
      "description": "MPV Tauri",
      "args": [
        {
          "name": "source",
          "index": 1,
          "takesValue": true
        }
      ]
    }
  }
}