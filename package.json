{
  "name": "tauri-plugin-mpv-api",
  "version": "0.2.7",
  "author": "22",
  "description": "A Tauri plugin to embed the mpv player in your app via JSON IPC.",
  "license": "MIT",
  "type": "module",
  "homepage": "https://github.com/nini22P/tauri-plugin-mpv",
  "repository": {
    "type": "git",
    "url": "https://github.com/nini22P/tauri-plugin-mpv.git"
  },
  "bugs": {
    "url": "https://github.com/nini22P/tauri-plugin-mpv/issues"
  },
  "keywords": [
    "tauri",
    "plugin",
    "mpv",
    "media",
    "player",
    "video",
    "audio",
    "rust",
    "typescript"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "types": "./dist-js/index.d.ts",
  "main": "./dist-js/index.cjs",
  "module": "./dist-js/index.js",
  "exports": {
    "types": "./dist-js/index.d.ts",
    "import": "./dist-js/index.js",
    "require": "./dist-js/index.cjs"
  },
  "files": [
    "dist-js",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "example": "npm install && npm run build && cd examples/react && npm install && npm run tauri dev",
    "build": "rollup -c",
    "build:example": "npm install && npm run build && cd examples/react && npm install && npm run tauri build",
    "prepublishOnly": "npm run build",
    "pretest": "npm run build"
  },
  "dependencies": {
    "@tauri-apps/api": "2.8.0"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "12.1.4",
    "rollup": "^4.50.1",
    "tslib": "^2.6.2",
    "typescript": "^5.3.3"
  }
}